@charset "utf-8";

* { box-sizing: border-box; }

html, body { font-family: Helvetica, Arial, sans-serif; background: rgb(255=
, 255, 255); margin: 0px; }

h1 { font-size: 30px; }

p { font-size: 16px; font-weight: 100; line-height: 24px; }

.wrapper { padding-right: 20px; padding-left: 20px; padding-top: 50px; max-=
width: 580px; margin: auto; }

.version { border: 2px solid rgb(238, 238, 238); margin-top: 25px; height: =
200px; display: flex; }

.version-section { padding: 10px; height: 100%; }

.version-section.dark { background: rgb(21, 21, 19); }

.demo { border-right: 2px solid rgb(238, 238, 238); position: relative; wid=
th: 200px; }

.code { vertical-align: top; width: 336px; }

.code textarea { border: 2px solid rgb(238, 238, 238); outline: 0px; height=
: 100%; width: 100%; font-family: monospace; font-size: 10px; }

.github-corner:hover .octo-arm { animation: 560ms ease-in-out 0s 1 normal n=
one running octocat-wave; }

@keyframes octocat-wave {=20
  0% { transform: rotate(0deg); }
  20% { transform: rotate(-25deg); }
  40% { transform: rotate(10deg); }
  60% { transform: rotate(-25deg); }
  80% { transform: rotate(10deg); }
  100% { transform: rotate(0deg); }
}

@media (max-width: 500px) {
  .github-corner:hover .octo-arm { animation: 0s ease 0s 1 normal none runn=
ing none; }
  .github-corner .octo-arm { animation: 560ms ease-in-out 0s 1 normal none =
running octocat-wave; }
}

footer { width: 100%; margin-top: 25px; margin-bottom: 100px; background: r=
gb(243, 243, 243); height: 50px; padding: 15px 25px; color: rgb(45, 45, 45)=
; font-size: 13px; letter-spacing: 1px; font-family: monospace; }

footer a { color: rgb(45, 45, 45); }

footer span { margin-left: 10px; margin-right: 10px; }

.twitter-share-button { margin-bottom: -4px; }

@media (max-width: 500px) {
  footer { text-align: center; height: auto; line-height: 30px; }
  footer span { display: none; }
  footer a { display: block; }
  .twitter-share-button { margin-bottom: -8px; }
}
